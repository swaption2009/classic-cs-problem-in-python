   "source": [
    "def fib(n: int) -> int:\n",
    "    sum = 0\n",
    "    if n < 2:\n",
    "        return n\n",
    "    while n > 0:\n",
    "        sum = fib(n - 1) + fib(n - 2)\n",
    "        n -= 1\n",
    "    return sum"
   ]
  },
